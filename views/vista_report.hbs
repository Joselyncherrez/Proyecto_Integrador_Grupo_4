<script>
    /*SCRIPT PARA COONVERTIR TABLA A PDF*/
    function funcpdf() {
        var tabla = document.getElementById('tabla_principal').innerHTML;

            var style = "<style>";
            style = style + "table {width: 100%;font: 17px Calibri;}";
            style = style + "table, th, td {border: solid 1px #DDD; border-collapse: collapse;";
            style = style + "padding: 2px 3px;text-align: center;}";
            style = style + "</style>";
            
            var ventana = window.open('', '', 'height=700,width=700');

            ventana.document.write('<html><head>');
            ventana.document.write('<title>Tabla Concesionaria</title>');   
            ventana.document.write(style);          
            ventana.document.write('</head>');
            ventana.document.write('<body>');
            ventana.document.write(tabla);         
            ventana.document.write('</body></html>');

            ventana.document.close(); 

            ventana.print(); 
        }
    /*FIN SCRIPT PARA COONVERTIR TABLA A PDF*/
</script>
